{
  // Verwendet IntelliSense zum Ermitteln möglicher Attribute.
  // Zeigen Sie auf vorhandene Attribute, um die zugehörigen Beschreibungen anzuzeigen.
  // Weitere Informationen finden Sie unter https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Launch Program",
      "program": "${workspaceFolder}/js/src/chains/nameResolver.js",
      "sourceMaps": true,
      "outFiles": [
        "${workspaceRoot}/js/**/*.js"
      ]
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Mocha Tests",
      "program": "${workspaceFolder}/node_modules/mocha/bin/_mocha",
      "args": [
        "-u",
        "tdd",
        "--timeout",
        "999999",
        "--colors",
        "--grep",
        "check auto update nodelist",
        "${workspaceFolder}/js/test/**/*.js"
      ],
      "outFiles": [
        "${workspaceRoot}/js/**/*.js"
      ],
      "sourceMaps": true,
      "internalConsoleOptions": "openOnSessionStart",
      "env": {
        "CI": "true",
        "PATH": "/Users/simon/.cargo/bin:/opt/local/bin:/opt/local/sbin:/usr/local/bin:/Users/simon/bin:/Users/simon/.cargo/bin:/usr/bin:/bin:/usr/sbin:/sbin"
      },
      "preLaunchTask": "tsc"
    },
    {
      "type": "node",
      "request": "attach",
      "name": "Attach",
      "port": 9229
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Server starten",
      "program": "${workspaceFolder}/js/src/server/server.js",
      "args": [
        "--privateKey=0x2817fc5d9d8b9753b6d9888e2cb411c022feb06f416a51e79006e8f89984aac3",
        "--chain=0x7d0",
        "--rpcUrl=https://rpc.slock.it/kovan",
        "--registryRPC=https://rpc.slock.it/kovan",
        "--minBlockHeight=6",
        "--registry=0xf0fb87f4757c77ea3416afe87f36acaa0496c7e9",
        "--persistentFile=false",
        "--handler=ipfs",
        "--ipfsUrl=http://188.68.54.252:5001"
      ],
      "outFiles": [
        "${workspaceFolder}/js/**/*.js"
      ]
    },
    {
      "type": "node",
      "request": "launch",
      "name": "testClient starten",
      "program": "${workspaceFolder}/js/src/testClient.js",
      "outFiles": [
        "${workspaceFolder}/js/**/*.js"
      ]
    },
    {
      "type": "node",
      "request": "launch",
      "name": "cbor starten",
      "program": "${workspaceFolder}/js/src/types/cbor.js",
      "outFiles": [
        "${workspaceFolder}/js/**/*.js"
      ]
    }
  ]
}