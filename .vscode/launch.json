{
  // Verwendet IntelliSense zum Ermitteln möglicher Attribute.
  // Zeigen Sie auf vorhandene Attribute, um die zugehörigen Beschreibungen anzuzeigen.
  // Weitere Informationen finden Sie unter https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Launch Program",
      "program": "${workspaceFolder}/js/src/chains/nameResolver.js",
      "sourceMaps": true,
      "outFiles": [
        "${workspaceRoot}/js/**/*.js"
      ]
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Mocha Tests",
      "program": "${workspaceFolder}/node_modules/mocha/bin/_mocha",
      "args": [
        "-u",
        "tdd",
        "--timeout",
        "999999",
        "--colors",
        "--grep",
        "check auto update nodelist",
        "${workspaceFolder}/js/test/**/*.js"
      ],
      "outFiles": [
        "${workspaceRoot}/js/**/*.js"
      ],
      "sourceMaps": true,
      "internalConsoleOptions": "openOnSessionStart",
      "env": {
        "CI": "true",
        "PATH": "/Users/simon/.cargo/bin:/opt/local/bin:/opt/local/sbin:/usr/local/bin:/Users/simon/bin:/Users/simon/.cargo/bin:/usr/bin:/bin:/usr/sbin:/sbin"
      },
      "preLaunchTask": "tsc"
    },
    {
      "type": "node",
      "request": "attach",
      "name": "Attach",
      "port": 9229
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Server starten",
      "program": "${workspaceFolder}/js/src/server/server.js",
      "args": [
        "--chain=0x44d",
        "--privateKey=0xdb2d2790fce3a4fb45427436b4d049539e74619a1543672023b4fa2a2205f994",
        "--rpcUrl=https://rpc.slock.it/tobalaba",
        "--registryRPC=https://rpc.slock.it/tobalaba",
        "--minBlockHeight=0",
        "--registry=0x845E484b505443814B992Bf0319A5e8F5e407879",
        "--persistentFile=false"
      ],
      "outFiles": [
        "${workspaceFolder}/js/**/*.js"
      ]
    },
    {
      "type": "node",
      "request": "launch",
      "name": "testClient starten",
      "program": "${workspaceFolder}/js/src/testClient.js",
      "outFiles": [
        "${workspaceFolder}/js/**/*.js"
      ]
    },
    {
      "type": "node",
      "request": "launch",
      "name": "cbor starten",
      "program": "${workspaceFolder}/js/src/types/cbor.js",
      "outFiles": [
        "${workspaceFolder}/js/**/*.js"
      ]
    }
  ]
}