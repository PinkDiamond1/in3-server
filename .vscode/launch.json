{
  // Verwendet IntelliSense zum Ermitteln möglicher Attribute.
  // Zeigen Sie auf vorhandene Attribute, um die zugehörigen Beschreibungen anzuzeigen.
  // Weitere Informationen finden Sie unter https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Launch Program",
      "program": "${workspaceFolder}/js/src/chains/nameResolver.js",
      "sourceMaps": true,
      "outFiles": [
        "${workspaceRoot}/js/**/*.js"
      ]
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Mocha Tests",
      "program": "${workspaceFolder}/node_modules/mocha/bin/_mocha",
      "args": [
        "-u",
        "tdd",
        "--timeout",
        "999999",
        "--colors",
        "--grep",
        "check update",
        "${workspaceFolder}/js/test/*/*.js"
      ],
      "outFiles": [
        "${workspaceRoot}/js/**/*.js"
      ],
      "sourceMaps": true,
      "internalConsoleOptions": "openOnSessionStart",
      "env": {
        "PATH": "/Users/simon/.cargo/bin:/opt/local/bin:/opt/local/sbin:/usr/local/bin:/Users/simon/bin:/Users/simon/.cargo/bin:/usr/bin:/bin:/usr/sbin:/sbin"
      },
      "preLaunchTask": "tsc"
    },
    {
      "type": "node",
      "request": "attach",
      "name": "Attach",
      "port": 9229
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Server starten",
      "program": "${workspaceFolder}/js/src/server/server.js",
      "args": [
        "--privateKey=0x1858a0f49ce12df65031ba0eb0b353abc74f93f8ccd43df9682fd2e2293a4db3",
        "--chain=0x2a",
        "--rpcUrl=https://kovan.infura.io/HVtVmCIHVgqHGUgihfhX",
        "--minBlockHeight=6",
        "--registry=0x013b82355a066A31427df3140C5326cdE9c64e3A",
        "--persistentFile=false",
        "--logging-host=logs7.papertrailapp.com",
        "--logging-name=PaperTrail",
        "--logging-port=30571",
        "--logging-type=winston-papertrail"
      ],
      "outFiles": [
        "${workspaceFolder}/js/**/*.js"
      ]
    },
    {
      "type": "node",
      "request": "launch",
      "name": "testClient starten",
      "program": "${workspaceFolder}/js/src/testClient.js",
      "outFiles": [
        "${workspaceFolder}/js/**/*.js"
      ]
    },
    {
      "type": "node",
      "request": "launch",
      "name": "cbor starten",
      "program": "${workspaceFolder}/js/src/types/cbor.js",
      "outFiles": [
        "${workspaceFolder}/js/**/*.js"
      ]
    }
  ]
}