{
  "name": "in3-server",
  "version": "0.1.0",
  "description": "Typescript-version of the in3 server",
  "main": "js/src/server/server.js",
  "scripts": {
    "docu": "typedoc  --exclude test --excludePrivate  --readme none --ignoreCompilerErrors --target ES6  --mode 'modules' --json doc.json src/index.ts ; cat  doc.json | slockit-docu index slockit/in3-server/blob/master/src \"#INCUBED API\" >  src/README.md ; rm doc.json",
    "updateIn3": "rm -rf node_modules/in3 package-lock.json; npm install",
    "build": "tsc -p .",
    "testAll": "cd node_modules/in3; npm run build && npm test; cd ../..; npm test",
    "test": "mocha --timeout 5000 -R in3/js/test/util/mochaReporter --require ts-node/register --require source-map-support/register  --full-trace --bail  test/*/*.ts",
    "testCoverage": "nyc mocha --timeout 5000 -R in3/js/test/util/mochaReporter --require ts-node/register --require source-map-support/register  --full-trace --bail  test/*/*.ts && cp -r coverage test/report/",
    "updateContracts": "docker run -v $(pwd)/contracts:/contracts ethereum/solc:stable --optimize --combined-json abi,bin,bin-runtime,compact-format,hashes,interface,metadata /contracts/ServerRegistry.sol /contracts/ChainRegistry.sol > contracts/contracts.json; solc --optimize --combined-json abi,bin,bin-runtime,compact-format,hashes,interface,metadata test/contracts/*.sol > test/contracts/contracts.json",
    "start": "node --inspect js/server/server",
    "parity": "docker run -p 8180:8180 -p 8545:8545 -p 8546:8546 slockit/parity-in3:latest --jsonrpc-apis all --logging=3 --jsonrpc-interface=all --ws-interface=all ---chain dev --gas-cap=8000000 --gasprice 0  --reseal-min-period 0   --gas-floor-target 8700000"
  },
  "nyc": {
    "include": [
      "node_modules/in3/src/**/*.ts",
      "node_modules/in3/js/src/**/*.js",
      "src/**/*.ts",
      "test/**/*.ts"
    ],
    "exclude": [
      "!node_modules/in3/js/src/**/*.js",
      "test/**/*.ts"
    ],
    "extension": [
      ".ts"
    ],
    "require": [
      "ts-node/register"
    ],
    "reporter": [
      "text-summary",
      "html"
    ],
    "sourceMap": true,
    "instrument": true
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/slockit/in3-server.git"
  },
  "author": "Simon Jentzsch",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/slockit/in3-server/issues"
  },
  "homepage": "https://github.com/slockit/in3-server#readme",
  "devDependencies": {
    "@babel/core": "^7.0.0-beta.49",
    "@babel/preset-env": "^7.0.0-beta.49",
    "@babel/preset-es2015": "^7.0.0-beta.49",
    "@types/cbor": "^2.0.0",
    "@types/chai": "^4.1.3",
    "@types/koa": "^2.0.44",
    "@types/koa-bodyparser": "^4.2.0",
    "@types/koa-router": "^7.0.27",
    "@types/mocha": "^5.2.0",
    "@types/winston": "^2.3.8",
    "chai": "^4.1.2",
    "dtsgenerator": "^0.9.9",
    "mocha": "^5.1.1",
    "nyc": "^11.8.0",
    "slockit-generator": "git+ssh://git@github.com/slockit/generator.git#master",
    "source-map-support": "^0.5.6",
    "ts-node": "^6.0.3",
    "typedoc": "^0.11.1",
    "typescript": "^2.9.1"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "cbor": "^4.0.0",
    "cli-color": "^1.2.0",
    "ethereumjs-abi": "0.6.5",
    "ethereumjs-account": "2.0.5",
    "ethereumjs-tx": "1.3.4",
    "ethereumjs-util": "5.2.0",
    "ethereumjs-vm": "2.3.5",
    "ethereumjs-wallet": "^0.6.0",
    "ethjs-util": "0.1.4",
    "in3": "git+ssh://git@github.com/slockit/in3.git#master",
    "koa": "^2.5.0",
    "koa-bodyparser": "^4.2.0",
    "koa-router": "^7.4.0",
    "merkle-patricia-tree": "^2.3.1",
    "winston": "^2.4.2"
  }
}